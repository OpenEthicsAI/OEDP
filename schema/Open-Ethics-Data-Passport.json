{
  "$id": "https://openethics.ai/schemas/Open-Ethics-Data-Passport.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Open Ethics Data Passport Schema",
  "type": "object",
  "description": "OEDP Model",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "models": {
      "type": "array",
      "items": {
        "$ref": "./Model.json"
      }
    },
    "scope": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "product": {
      "type": [
        "array",
        "string"
      ],
      "format": "uuid",
      "items": {}
    },
    "updated": {
      "type": "string",
      "format": "date-time"
    },
    "validUntil": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "models",
    "scope",
    "created"
  ]
}
